
<div class="container">
  <div class="row">
      <%= form_for(@fine,class:"col s12") do |f| %>
       <% if fine.errors.any? %>
        <div id="error_explanation">
          <h2><%= pluralize(fine.errors.count, "error") %> prohibited this fine from being saved:</h2>

          <ul>
          <% fine.errors.full_messages.each do |message| %>
            <li><%= message %></li>
          <% end %>
          </ul>
        </div>
      <% end %>
        <div class="row">
          <div class="input-field col s6">
             <%= f.text_field :name %>
             <%= f.label :name %>
          </div>
          <div class="input-field col s6">
             <%= f.number_field :price %>
             <%= f.label :price %>
          </div>
        </div>
         <div class="row">
       
          <div class="input-field col s12">
             <%= f.text_area :description,id:"textarea1",class:"materialize-textarea" %>
             <%= f.label :description,for:"textarea1" %>
          </div>
        </div>
        <div class="actions">
         
          <%= f.submit class:'btn btn-large green' %>
        </div>
      <% end %>
    </div>
</div>